{
  "name": "f7-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devHost": "vite --host",
    "build": "tsc && vite build && concurrently \"cap sync android\" \"cap sync ios\"",
    "build:generateAssets": "capacitor-assets generate --android --ios",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:android": "cap run android --no-sync",
    "preview:ios": "cap run ios --no-sync"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.3",
    "@capacitor/app": "^5.0.2",
    "@capacitor/browser": "^5.0.2",
    "@capacitor/core": "^5.0.3",
    "@capacitor/ios": "^5.0.3",
    "@capacitor/keyboard": "^5.0.2",
    "@capacitor/splash-screen": "^5.0.2",
    "@capacitor/status-bar": "^5.0.2",
    "framework7": "^8.0.5",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.0.5",
    "konsta": "^2.0.0",
    "material-icons": "^1.13.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "skeleton-elements": "^4.0.1",
    "swiper": "^9.3.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.0.3",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "simple-zustand-devtools": "^1.1.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  }
}
